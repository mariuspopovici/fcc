{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": ["last 2 versions", "safari >= 7"]
      }
    ]
  ],
  "plugins": [["@babel/transform-runtime"]],
  "env": {
    "nyc": {
      "plugins": ["istanbul"]
    },
    "debug": {
      "sourceMaps": "inline",
      "retainLines": true
    }
  }
}
